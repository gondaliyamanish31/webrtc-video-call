{
  "name": "webrtc-video-call",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js}\"",
    "prepare": "husky install",
    "clean": "rm -rf node_modules .turbo && npx turbo run clean",
    "check:types": "turbo check:types",
    "check:format": "prettier --check \"**/*.{ts,tsx,js}\""
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.6",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "next-auth": "^4.24.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  }
}