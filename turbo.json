{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "**/*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "clean": {
      "cache": false,
      "persistent": true
    },
    "check:types": {
      "dependsOn": [
        "^check:types"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    }
  }
}